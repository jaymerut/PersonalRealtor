<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PersonalRealtor.Views.Pages.RealtorListings.UI.RealtorListingsPage"
             x:Name="RealtorListings">
    <ContentPage.Content>
        <StackLayout Spacing="0" BackgroundColor="Transparent" AbsoluteLayout.LayoutBounds="0, 0, 1, 1" AbsoluteLayout.LayoutFlags="All">
            <ScrollView x:Name="ScrollViewList" BackgroundColor="Transparent" VerticalOptions="FillAndExpand" Padding="14,20,14,0">
                <ListView x:Name="RealtorListingsListView" BackgroundColor="Transparent"/>
            </ScrollView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>